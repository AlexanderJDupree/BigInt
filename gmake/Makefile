# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug_x64
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_x64)
  BigInt_config = debug_x64
  Tests_config = debug_x64
endif
ifeq ($(config),debug_x86)
  BigInt_config = debug_x86
  Tests_config = debug_x86
endif
ifeq ($(config),release_x64)
  BigInt_config = release_x64
  Tests_config = release_x64
endif
ifeq ($(config),release_x86)
  BigInt_config = release_x86
  Tests_config = release_x86
endif

PROJECTS := BigInt Tests

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

BigInt:
ifneq (,$(BigInt_config))
	@echo "==== Building BigInt ($(BigInt_config)) ===="
	@${MAKE} --no-print-directory -C . -f BigInt.make config=$(BigInt_config)
endif

Tests: BigInt
ifneq (,$(Tests_config))
	@echo "==== Building Tests ($(Tests_config)) ===="
	@${MAKE} --no-print-directory -C . -f Tests.make config=$(Tests_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f BigInt.make clean
	@${MAKE} --no-print-directory -C . -f Tests.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_x64"
	@echo "  debug_x86"
	@echo "  release_x64"
	@echo "  release_x86"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   BigInt"
	@echo "   Tests"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"